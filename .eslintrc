{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "strict":[2,"never"],
    "react/jsx-uses-react":2,
    "react/jsx-uses-vars":2,
    "react/jsx-in-jsx-scope":2
  },
  "plugins":[
  "react"
  ]
}